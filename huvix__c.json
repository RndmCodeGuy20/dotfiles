{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf303 {{ .UserName }}ðŸ¤–{{ .HostName }} @",
          "foreground": "#BBC4B9",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "#56B6C2",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "(\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}) ",
          "foreground": "#7FD5EA",
          "type": "go",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "(\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}) ",
          "foreground": "#9e7eff",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "template": "<#E8CC97>git(</>{{ .HEAD }}<#E8CC97>) </>",
          "foreground": "#56B6C2",
          "type": "git",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "foreground": "#fff",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "dallas",
            "threshold": 0
          },
          "template": " <#BBC4B9>{{ .FormattedMs }}s</> <#fff>âš¡</>",
          "foreground": "#fff",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "<#BBC4B9>15:04:05</> <#fff>on</> 2 Jan, Monday"
          },
          "template": "{{ .CurrentDate | date .Format }} <#79DFE1></>",
          "foreground": "#F2D3B6",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "root_icon": "\uf292 "
          },
          "template": " \uf0e7",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " <#fff>\ue718</> {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#BBC4B9",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\ue738 {{ if .Error }}{{ 7 }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ec2729",
          "type": "java",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "charged_icon": "\ue22f ",
            "charging_icon": "\ue234 ",
            "discharging_icon": "\ue231 "
          },
          "template": "<#BBC4B9>[</>{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 ",
          "foreground": "#193549",
          "type": "battery",
          "style": "powerline",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#64B5F6{{end}}",
            "{{if eq \"Discharging\" .State.String}}#E36464{{end}}",
            "{{if eq \"Full\" .State.String}}#66BB6A{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB<#BBC4B9>] âžœ</>",
          "foreground": "#BBC4B9",
          "type": "sysinfo",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "playing_icon": "\uf1bc "
          },
          "template": " {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} ~ {{ .Track }}{{ end }} ",
          "foreground": "#1DB954",
          "type": "spotify",
          "style": "diamond"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
